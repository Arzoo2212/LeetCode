class Solution {
public:
    int mySqrt(int x) {
       int s=1;
       int e=x;
        int ans=0;
       while(s<=e){
        long mid=s+(e-s)/2;
        long sq=mid*mid;
       
         if((sq)==x){
            return mid;
         }
         else if(sq>x){
            e=mid-1;
         }
         else if(sq<x){
            s=mid+1;
            ans=mid;
         }  
       }
       return ans;
    }
};
